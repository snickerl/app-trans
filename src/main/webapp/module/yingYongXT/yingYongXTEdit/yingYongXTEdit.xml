      <properties component="properties">
        <property component="simpleProperty" name="yingYongXTBH" label="应用系统编号" fieldType="string"/>
        <property component="simpleProperty" name="yingYongXTMC" label="应用系统名称" fieldType="string"/>
        <property component="simpleProperty" name="shuJuJKSXL" label="数据接口实现类" fieldType="string"/>
        <property component="simpleProperty" name="shiFouJSSJ" label="是否接收数据" fieldType="boolean"/>
        <property component="simpleProperty" name="shiFouCCSJ" label="是否传出数据" fieldType="boolean"/>

        <property component="simpleProperty" name="shiFouCCRW" label="是否传出任务" fieldType="boolean"/>
      </properties>
    </record>
    <fields component="formFields">
      <field fieldType="string" component="stringEditor" name="yingYongXTBH" label="应用系统编号" renderType="insert" width="120px" height="22px" renderto="#shiTiLeiDH" renderTemplate="{{yingYongXTBH}}" enabled="true" allowEdit="true" allowBlank="false" hidden="false"/>
